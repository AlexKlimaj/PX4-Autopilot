#!/bin/sh
#
# board sensors init
#------------------------------------------------------------------------------
icm42688p -R 0 -s start

gps start -d /dev/ttyS0 -p ubx

# probe for optional external I2C devices
icm20948_i2c_passthrough -X -q start

# compasses
hmc5883 -T -X -q start
ist8308 -X -q start
ist8310 -X -q start
lis2mdl -X -q start
lis3mdl -X -q start
qmc5883l -X -q start
rm3100 -X -q start

# start last (wait for possible icm20948 passthrough mode)
ak09916 -X -q start
